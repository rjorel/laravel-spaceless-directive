{
    "name": "rjorel/laravel-spaceless-blade",
    "description": "Spaceless directive for Blade",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Raphael Jorel",
            "email": "raphael.jorel@laposte.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Rjorel\\LaravelSpacelessBlade\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rjorel\\LaravelSpacelessBlade\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rjorel\\LaravelSpacelessBlade\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
